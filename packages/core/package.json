{
        "name": "glre",
        "version": "0.17.0",
        "author": "tseijp",
        "license": "MIT",
        "private": false,
        "sideEffect": false,
        "publishConfig": {
                "access": "public"
        },
        "bugs": {
                "url": "https://github.com/tseijp/glre/issues"
        },
        "repository": "tseijp/glre",
        "homepage": "https://glre.tsei.jp",
        "keywords": [
                "glsl",
                "webgl",
                "hooks",
                "react",
                "reactjs",
                "reactive",
                "solid",
                "solidjs",
                "typescript"
        ],
        "main": "./dist/index.js",
        "types": "./dist/index.d.mts",
        "module": "./dist/index.mjs",
        "exports": {
                "./packages.json": "./packages.json",
                ".": {
                        "types": "./dist/index.d.mts",
                        "import": {
                                "types": "./dist/index.d.mts",
                                "default": "./dist/index.mjs"
                        },
                        "module": {
                                "types": "./dist/index.d.mts",
                                "default": "./dist/index.mjs"
                        },
                        "require": "./dist/index.js",
                        "default": "./dist/index.js"
                },
                "./*": {
                        "types": "./dist/index.d.mts",
                        "import": {
                                "types": "./dist/index.d.mts",
                                "default": "./dist/index.mjs"
                        },
                        "module": {
                                "types": "./dist/index.d.mts",
                                "default": "./dist/index.mjs"
                        },
                        "require": "./dist/index.js",
                        "default": "./dist/index.js"
                },
                "./react": {
                        "types": "./dist/react.d.mts",
                        "import": {
                                "types": "./dist/react.d.mts",
                                "default": "./dist/react.mjs"
                        },
                        "module": {
                                "types": "./dist/react.d.mts",
                                "default": "./dist/react.mjs"
                        },
                        "require": "./dist/react.js",
                        "default": "./dist/react.js"
                },
                "./react/*": {
                        "types": "./dist/react.d.mts",
                        "import": {
                                "types": "./dist/react.d.mts",
                                "default": "./dist/react.mjs"
                        },
                        "module": {
                                "types": "./dist/react.d.mts",
                                "default": "./dist/react.mjs"
                        },
                        "require": "./dist/react.js",
                        "default": "./dist/react.js"
                },
                "./native": {
                        "types": "./dist/native.d.mts",
                        "import": {
                                "types": "./dist/native.d.mts",
                                "default": "./dist/native.mjs"
                        },
                        "module": {
                                "types": "./dist/native.d.mts",
                                "default": "./dist/native.mjs"
                        },
                        "require": "./dist/native.js",
                        "default": "./dist/native.js"
                },
                "./native/*": {
                        "types": "./dist/native.d.mts",
                        "import": {
                                "types": "./dist/native.d.mts",
                                "default": "./dist/native.mjs"
                        },
                        "module": {
                                "types": "./dist/native.d.mts",
                                "default": "./dist/native.mjs"
                        },
                        "require": "./dist/native.js",
                        "default": "./dist/native.js"
                },
                "./solid": {
                        "types": "./dist/solid.d.mts",
                        "import": {
                                "types": "./dist/solid.d.mts",
                                "default": "./dist/solid.mjs"
                        },
                        "module": {
                                "types": "./dist/solid.d.mts",
                                "default": "./dist/solid.mjs"
                        },
                        "require": "./dist/solid.js",
                        "default": "./dist/solid.js"
                },
                "./solid/*": {
                        "types": "./dist/solid.d.mts",
                        "import": {
                                "types": "./dist/solid.d.mts",
                                "default": "./dist/solid.mjs"
                        },
                        "module": {
                                "types": "./dist/solid.d.mts",
                                "default": "./dist/solid.mjs"
                        },
                        "require": "./dist/solid.js",
                        "default": "./dist/solid.js"
                }
        },
        "dependencies": {
                "reev": "0.11.0",
                "refr": "0.3.0"
        },
        "peerDependencies": {
                "expo": "*",
                "expo-gl": "*",
                "react": ">=16.8",
                "react-dom": ">=16.8",
                "solid-js": "*"
        },
        "peerDependenciesMeta": {
                "expo": {
                        "optional": true
                },
                "expo-gl": {
                        "optional": true
                },
                "react": {
                        "optional": true
                },
                "react-dom": {
                        "optional": true
                },
                "solid-js": {
                        "optional": true
                }
        }
}
