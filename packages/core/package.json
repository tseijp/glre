{
        "name": "glre",
        "version": "0.35.0",
        "author": "tseijp",
        "license": "MIT",
        "private": false,
        "sideEffect": false,
        "type": "module",
        "publishConfig": {
                "access": "public"
        },
        "bugs": {
                "url": "https://github.com/tseijp/glre/issues"
        },
        "repository": "tseijp/glre",
        "homepage": "https://glre.tsei.jp",
        "keywords": [
                "glsl",
                "webgl",
                "hooks",
                "react",
                "reactjs",
                "reactive",
                "solid",
                "solidjs",
                "typescript"
        ],
        "main": "./dist/index.cjs",
        "types": "./dist/index.d.ts",
        "module": "./dist/index.js",
        "exports": {
                "./packages.json": "./packages.json",
                "./src/*": {
                        "default": "./src/*.ts"
                },
                "./react": {
                        "types": "./dist/react.d.ts",
                        "import": {
                                "types": "./dist/react.d.ts",
                                "default": "./dist/react.js"
                        },
                        "module": {
                                "types": "./dist/react.d.ts",
                                "default": "./dist/react.js"
                        },
                        "require": "./dist/react.cjs",
                        "default": "./dist/react.js"
                },
                "./native": {
                        "types": "./dist/native.d.ts",
                        "import": {
                                "types": "./dist/native.d.ts",
                                "default": "./dist/native.js"
                        },
                        "module": {
                                "types": "./dist/native.d.ts",
                                "default": "./dist/native.js"
                        },
                        "require": "./dist/native.cjs",
                        "default": "./dist/native.js"
                },
                "./solid": {
                        "types": "./dist/solid.d.ts",
                        "import": {
                                "types": "./dist/solid.d.ts",
                                "default": "./dist/solid.js"
                        },
                        "module": {
                                "types": "./dist/solid.d.ts",
                                "default": "./dist/solid.js"
                        },
                        "require": "./dist/solid.cjs",
                        "default": "./dist/solid.js"
                },
                ".": {
                        "types": "./dist/index.d.ts",
                        "import": {
                                "types": "./dist/index.d.ts",
                                "default": "./dist/index.js"
                        },
                        "module": {
                                "types": "./dist/index.d.ts",
                                "default": "./dist/index.js"
                        },
                        "require": "./dist/index.cjs",
                        "default": "./dist/index.js"
                },
                "./*": {
                        "types": "./dist/index.d.ts",
                        "import": {
                                "types": "./dist/index.d.ts",
                                "default": "./dist/index.js"
                        },
                        "module": {
                                "types": "./dist/index.d.ts",
                                "default": "./dist/index.js"
                        },
                        "require": "./dist/index.cjs",
                        "default": "./dist/index.js"
                }
        },
        "files": [
                "dist/**/*",
                "src/**/*",
                "packages.json",
                "README.md"
        ],
        "dependencies": {
                "@webgpu/types": "*",
                "reev": "0.12.0",
                "refr": "0.7.0"
        },
        "peerDependencies": {
                "expo": "*",
                "expo-gl": "*",
                "react": ">=16.8",
                "react-dom": ">=16.8",
                "react-native": "*",
                "solid-js": "*"
        },
        "peerDependenciesMeta": {
                "expo": {
                        "optional": true
                },
                "expo-gl": {
                        "optional": true
                },
                "react": {
                        "optional": true
                },
                "react-dom": {
                        "optional": true
                },
                "react-native": {
                        "optional": true
                },
                "solid-js": {
                        "optional": true
                }
        }
}
